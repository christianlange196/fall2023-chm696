\begin_layout Description
Regular
\begin_inset space ~
\end_inset

representation 
\begin_inset Formula 
\[
\begin{split}\dot{\rho}_{ee} & =-\Gamma\rho_{ee}+i\frac{\Omega}{2}\left(\rho_{eg}-\rho_{ge}\right)\\
\dot{\rho}_{gg} & =1-\rho_{ee}\\
\dot{\rho}_{eg} & =-\left(\gamma_{\perp}-i\Delta\right)\rho_{eg}+i\frac{\Omega}{2}\left(\rho_{ee}-\rho_{gg}\right)\\
\dot{\rho}_{ge} & =-\left(\gamma_{\perp}+i\Delta\right)\rho_{ge}-i\frac{\Omega}{2}\left(\rho_{ee}-\rho_{gg}\right).
\end{split}
\]

\end_inset


\end_layout

\begin_layout Description
Bloch-vector
\begin_inset space ~
\end_inset

representation 
\begin_inset Formula 
\[
\begin{aligned}\partial_{t}\left\langle \sigma_{x}\right\rangle  & =\Delta\left\langle \sigma_{y}\right\rangle -\gamma_{\perp}\left\langle \sigma_{x}\right\rangle \\
\partial_{t}\left\langle \sigma_{y}\right\rangle  & =-\Delta\left\langle \sigma_{x}\right\rangle -\Omega\left\langle \sigma_{z}\right\rangle -\gamma_{\perp}\left\langle \sigma_{y}\right\rangle \\
\partial_{t}\left\langle \sigma_{z}\right\rangle  & =\Omega\left\langle \sigma_{y}\right\rangle -\Gamma\left(\left\langle \sigma_{z}\right\rangle +1\right)
\end{aligned}
\]

\end_inset

where 
\begin_inset Formula 
\[
\begin{split}\left\langle \sigma_{x}\right\rangle  & =\rho_{eg}+\rho_{ge}\\
\left\langle \sigma_{y}\right\rangle  & =i\left(\rho_{eg}-\rho_{ge}\right)\\
\left\langle \sigma_{z}\right\rangle  & =\rho_{ee}-\rho_{gg}
\end{split}
\]

\end_inset


\end_layout

\begin_layout Description
Example:
\begin_inset space ~
\end_inset

no
\begin_inset space ~
\end_inset

drive If we take 
\begin_inset Formula $\Delta=0$
\end_inset

 and 
\begin_inset Formula $\Omega=0$
\end_inset

,
\begin_inset Formula 
\begin{align*}
\partial_{t}\left\langle \sigma_{x}\right\rangle  & =-\gamma_{\perp}\left\langle \sigma_{x}\right\rangle \\
\partial_{t}\left\langle \sigma_{y}\right\rangle  & =-\gamma_{\perp}\left\langle \sigma_{y}\right\rangle \\
\partial_{t}\left\langle \sigma_{z}\right\rangle  & =-\Gamma\left(\left\langle \sigma_{z}\right\rangle +1\right)
\end{align*}

\end_inset

with initial condition 
\begin_inset Formula $\left\langle \sigma_{z}\right\rangle =1$
\end_inset

, we have 
\begin_inset Formula 
\[
\left\langle \sigma_{z}\right\rangle =2e^{-\Gamma t}-1.
\]

\end_inset


\end_layout

